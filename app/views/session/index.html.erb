<div class="container">

  
  <% if notice %>
    <%= notice %>
  <% end %>
  
  <%= form_with model: @user, local: true do |form| %>
    <div class="form-group">
      <%= form.label "session por Fone" %>
      <%= form.text_field :phone, class: "form-control"%>
    </div>

    <div class="actions">
      <%= form.submit "Log In", class: "btn btn-success"%>
    </div>
  <% end %>

  

  <div>
    <%= link_to 'Novo usuario', new_user_path, class:"btn btn-primary" %>
  </div>
</div>